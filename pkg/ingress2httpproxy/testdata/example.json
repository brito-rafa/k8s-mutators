{
    "apiVersion": "networking.k8s.io/v1beta1",
    "kind": "Ingress",
    "metadata": {
      "name": "cafe-ingress",
      "annotations": {
      "ingress.kubernetes.io/proxy-body-size": "50m",
      "ingress2httpproxy/fqdnnewdomain": "sample.vmware.com"
      
    }
    },
    "spec": {
      "tls": [
        {
          "hosts": [
            "cafe.example.com"
          ],
          "secretName": "cafe-secret"
         
        }
      ],
      "rules": [
        {
          "host": "cafe.vmware.com",
          "http": {
            "paths": [
              {
                "path": "/tea",
                "backend": {
                  "serviceName": "tea-svc",
                  "servicePort": 80
                }
              },
              {
                "path": "/coffee",
                "backend": {
                  "serviceName": "coffee-svc",
                  "servicePort": 80
                }
              }
            ]
          }
        },
        {
          "host": "cafe2.vmware.com",
          "http": {
            "paths": [
              {
                "path": "/tea",
                "backend": {
                  "serviceName": "tea-svc",
                  "servicePort": 80
                }
              },
              {
                "path": "/coffee",
                "backend": {
                  "serviceName": "coffee-svc",
                  "servicePort": 80
                }
              }
            ]
          }
        },{
          "host": "cafe3.vmware.com",
          "http": {
            "paths": [
              {
                "path": "/tea",
                "backend": {
                  "serviceName": "tea-svc",
                  "servicePort": 80
                }
              },
              {
                "path": "/coffee",
                "backend": {
                  "serviceName": "coffee-svc",
                  "servicePort": 80
                }
              }
            ]
          }
        }
        
            ]
          
    }
  }